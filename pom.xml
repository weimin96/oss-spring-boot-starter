<!--                    <artifactId>maven-gpg-plugin</artifactId>-->
<!--                    <version>1.6</version>-->
<!--                    <configuration>-->
<!--                        <gpgArguments>-->
<!--                            <arg>&#45;&#45;pinentry-mode</arg>-->
<!--                            <arg>loopback</arg>-->
<!--                        </gpgArguments>-->
<!--                    </configuration>-->
<!--                    <executions>-->
<!--                        <execution>-->
<!--                            <id>sign-artifacts</id>-->
